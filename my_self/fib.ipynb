    "def fib(n):\n",
    "    if n < 2:\n",
    "        return n\n",
    "    else:\n",
    "        n_1 = 0\n",
    "        n_2 = 1\n",
    "        count = 1\n",
    "        while count < n:\n",
    "            n_1, n_2 = n_2, n_1 + n_2\n",
    "            count += 1\n",
    "        return n_2"
   ]
